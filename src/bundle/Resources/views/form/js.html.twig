<script>
    let maskedInputs = document.getElementsByClassName("mateuszbieniek-masked-input");
    let i;
    for (i = 0; i < maskedInputs.length; i++) {
        let maskedInput = maskedInputs[i];
        let im = new Inputmask({"mask": maskedInput.getAttribute("mask"), "greedy": maskedInput.getAttribute("greedy")});
        im.mask(maskedInput);
    }
</script>